                        </m:mrow>
                        <m:mi>x</m:mi>
                      </m:mfrac>
                    </m:msqrt>
                    <m:mspace width="0.166667em"/>
                    <m:mfenced separators="" open="[" close="]">
                      <m:mrow>
                        <m:mi>η</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mo>'</m:mo>
                        </m:msup>
                        <m:mo>-</m:mo>
                        <m:mi>f</m:mi>
                      </m:mrow>
                    </m:mfenced>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="rightt">
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>u</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>x</m:mi>
                      </m:mrow>
                    </m:mfrac>
</m:mtd>
<m:mtd>
                    <m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">       
             <m:mo>-</m:mo>
                    <m:mfrac>
                      <m:msub>
                        <m:mi>U</m:mi>
                        <m:mi>∞</m:mi>
                      </m:msub>
                      <m:mrow>
                        <m:mn>2</m:mn>
                        <m:mi>x</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mspace width="0.166667em"/>
                    <m:mi>η</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                      </m:mrow>
                    </m:msup>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">                                      <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>u</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
</m:mtd>
<m:mtd>
                    <m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">       
             <m:msub>
                      <m:mi>U</m:mi>
                      <m:mi>∞</m:mi>
                    </m:msub>
                    <m:mspace width="0.166667em"/>
                    <m:msqrt>
                      <m:mfrac>
                        <m:msub>
                          <m:mi>U</m:mi>
                          <m:mi>∞</m:mi>
                        </m:msub>
                        <m:mrow>
                          <m:mi>ν</m:mi>
                          <m:mspace width="0.166667em"/>
                          <m:mi>x</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:msqrt>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                      </m:mrow>
                    </m:msup>
               </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
               
                    <m:mfrac>
                      <m:mrow>
                        <m:msup>
                          <m:mi>∂</m:mi>
                          <m:mn>2</m:mn>
                        </m:msup>
                        <m:mi>u</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msup>
                          <m:mi>y</m:mi>
                          <m:mn>2</m:mn>
                        </m:msup>
                      </m:mrow>
                    </m:mfrac>
</m:mtd>
<m:mtd>                    
<m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">       
             <m:mfrac>
                      <m:msubsup>
                        <m:mi>U</m:mi>
                        <m:mi>∞</m:mi>
                        <m:mn>2</m:mn>
                      </m:msubsup>
                      <m:mrow>
                        <m:mi>ν</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:mi>x</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                      </m:mrow>
                    </m:msup>
                </m:mtd>
              </m:mtr>
            </m:mtable>
          </m:math>
        </equation><para id="id2293641">Substituting the above equations into the equation of motion and cancellation of two terms results in the following ordinary differential equation.</para>
        
        <equation id="eip-36"><m:math overflow="scroll">
            <m:mtable>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                      </m:mrow>
                    </m:msup>
                    <m:mo>+</m:mo>
                    <m:mi>f</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mo>'</m:mo>
                        <m:mo>'</m:mo>
                      </m:mrow>
                    </m:msup>
                    <m:mo>=</m:mo>
                    <m:mn>0</m:mn>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>f</m:mi>
                    <m:mfenced open="(" close=")">
                      <m:mn>0</m:mn>
                    </m:mfenced>
                    <m:mo>=</m:mo>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mo>'</m:mo>
                    </m:msup>
                    <m:mfenced open="(" close=")">
                      <m:mn>0</m:mn>
                    </m:mfenced>
                    <m:mo>=</m:mo>
                    <m:mn>0</m:mn>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mo>'</m:mo>
                    </m:msup>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mrow>
                        <m:mi>η</m:mi>
                        <m:mo>→</m:mo>
                        <m:mi>∞</m:mi>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
            </m:mtable>
          </m:math>
        </equation><para id="id2293768">This is a third order ODE with two conditions at <m:math overflow="scroll"><m:mrow><m:mi>η</m:mi><m:mo>→</m:mo><m:mi>∞</m:mi></m:mrow></m:math>
and one condition at . It is convenient to solve it as a set of first order ODEs with initial conditions, two of which are specified and the third adjusted such as to satisfy the condition at infinity.</para>
        
        <equation id="eip-475"><m:math overflow="scroll">
            <m:mrow>
              <m:mi mathvariant="bold">Y</m:mi>
              <m:mo>=</m:mo>
              <m:mfenced separators="" open="[" close="]">
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:mi>f</m:mi>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>'</m:mo>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>'</m:mo>
                          <m:mo>'</m:mo>
                        </m:mrow>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                </m:mtable>
              </m:mfenced>
              <m:mo>,</m:mo>
              <m:mspace width="1.em"/>
              <m:mfrac>
                <m:mrow>
                  <m:mi>d</m:mi>
                  <m:mi mathvariant="bold">Y</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mi>d</m:mi>
                  <m:mi>η</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mfenced separators="" open="[" close="]">
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>'</m:mo>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>'</m:mo>
                          <m:mo>'</m:mo>
                        </m:mrow>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>'</m:mo>
                          <m:mo>'</m:mo>
                          <m:mo>'</m:mo>
                        </m:mrow>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                </m:mtable>
              </m:mfenced>
              <m:mo>=</m:mo>
              <m:mfenced separators="" open="[" close="]">
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>'</m:mo>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>'</m:mo>
                          <m:mo>'</m:mo>
                        </m:mrow>
                      </m:msup>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mi>f</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mrow>
                            <m:mo>'</m:mo>
                            <m:mo>'</m:mo>
                          </m:mrow>
                        </m:msup>
                        <m:mo>/</m:mo>
                        <m:mn>2</m:mn>
                      </m:mrow>
                    </m:mtd>
                  </m:mtr>
                </m:mtable>
              </m:mfenced>
              <m:mo>=</m:mo>
              <m:mfenced separators="" open="[" close="]">
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:msub>
                        <m:mi>Y</m:mi>
                        <m:mn>2</m:mn>
                      </m:msub>
