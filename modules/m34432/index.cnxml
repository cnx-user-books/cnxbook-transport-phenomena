                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
                    <m:mfrac>
                      <m:mi>μ</m:mi>
                      <m:mrow>
                        <m:mi>ρ</m:mi>
                        <m:msup>
                          <m:mrow>
                            <m:msub>
                              <m:mi>U</m:mi>
                              <m:mi>∞</m:mi>
