                      </m:mrow>
                    </m:msub>
                </m:mtd>
              </m:mtr>
              <m:mtr>
<m:mtd/>              
  <m:mtd>
                    <m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">        
            <m:mfrac>
                      <m:mn>2</m:mn>
                      <m:msqrt>
                        <m:mrow>
                          <m:mn>8</m:mn>
                          <m:mi>π</m:mi>
                        </m:mrow>
                      </m:msqrt>
                    </m:mfrac>
                    <m:mfrac>
                      <m:mn>1</m:mn>
                      <m:msqrt>
                        <m:mrow>
                          <m:msub>
                            <m:mi>U</m:mi>
