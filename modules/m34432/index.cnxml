                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mi>v</m:mi>
                    <m:mo>=</m:mo>
                    <m:mfrac>
                      <m:mrow>
