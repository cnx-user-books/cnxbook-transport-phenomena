                        </m:msup>
                      </m:mrow>
                    </m:mfrac>
                    <m:msub>
                      <m:mfenced separators="" open="" close="|">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>∂</m:mi>
                            <m:msub>
                              <m:mi>v</m:mi>
                              <m:mi>x</m:mi>
                            </m:msub>
                          </m:mrow>
                          <m:mrow>
                            <m:mi>∂</m:mi>
