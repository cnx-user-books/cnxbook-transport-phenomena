                </m:msubsup>
                <m:msub>
                  <m:mi>U</m:mi>
                  <m:mi>∞</m:mi>
                </m:msub>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mi> erf </m:mi>
              <m:mfenced separators="" open="(" close=")">
                <m:mfrac>
                  <m:mi>y</m:mi>
                  <m:msqrt>
                    <m:mrow>
                      <m:mn>8</m:mn>
                      <m:mi>ν</m:mi>
                      <m:mspace width="0.166667em"/>
                      <m:mi>x</m:mi>
                      <m:mo>/</m:mo>
                      <m:msub>
                        <m:mi>U</m:mi>
                        <m:mi>∞</m:mi>
                      </m:msub>
                    </m:mrow>
                  </m:msqrt>
                </m:mfrac>
              </m:mfenced>
            </m:mrow>
          </m:math>
        </equation><para id="id2292522">This suggests a similarity variable:</para>
        
        <equation id="eip-324"><m:math overflow="scroll">
            <m:mtable>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
