                    <m:mfrac>
                      <m:mi>y</m:mi>
                      <m:msqrt>
                        <m:mrow>
                          <m:mi>ν</m:mi>
                          <m:mspace width="0.166667em"/>
                          <m:mi>x</m:mi>
                          <m:mo>/</m:mo>
                          <m:msub>
                            <m:mi>U</m:mi>
                            <m:mi>∞</m:mi>
                          </m:msub>
                        </m:mrow>
                      </m:msqrt>
                    </m:mfrac>
                    <m:mo>=</m:mo>
                    <m:mi>y</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msqrt>
                      <m:mfrac>
                        <m:msub>
                          <m:mi>U</m:mi>
                          <m:mi>∞</m:mi>
                        </m:msub>
                        <m:mrow>
                          <m:mi>ν</m:mi>
                          <m:mspace width="0.166667em"/>
                          <m:mi>x</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:msqrt>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>η</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
