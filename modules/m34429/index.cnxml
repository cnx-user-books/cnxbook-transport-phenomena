                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>∂</m:mi>
                      <m:mfenced separators="" open="(" close=")">
                        <m:mrow>
                          <m:mi>ρ</m:mi>
