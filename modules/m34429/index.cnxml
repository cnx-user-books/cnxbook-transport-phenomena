                  <m:mi>t</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>+</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:mi>∂</m:mi>
                  <m:mi>u</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mi>∂</m:mi>
                  <m:mi>x</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mn>1</m:mn>
                <m:msub>
                  <m:mi>N</m:mi>
                  <m:mrow>
                    <m:mi>P</m:mi>
                    <m:mi>e</m:mi>
                  </m:mrow>
                </m:msub>
              </m:mfrac>
              <m:mfrac>
                <m:mrow>
                  <m:msup>
                    <m:mi>∂</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
