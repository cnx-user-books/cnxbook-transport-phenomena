                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mfenced separators="" open="" close="|">
                        <m:mrow>
                          <m:mover accent="true">
                            <m:mi>f</m:mi>
                            <m:mo>¯</m:mo>
                          </m:mover>
                          <m:mrow>
                            <m:mo>(</m:mo>
                            <m:msup>
                              <m:mi>a</m:mi>
                              <m:mn>2</m:mn>
                            </m:msup>
                            <m:mo>/</m:mo>
                            <m:mi>z</m:mi>
                            <m:mo>)</m:mo>
                          </m:mrow>
                        </m:mrow>
                      </m:mfenced>
                      <m:mrow>
                        <m:mfenced open="|" close="|">
                          <m:mi>z</m:mi>
                        </m:mfenced>
                        <m:mo>=</m:mo>
                        <m:mi>a</m:mi>
                      </m:mrow>
                    </m:msub>
                
                </m:mtd>
              </m:mtr>
              <m:mtr>
