        </equation>
        <equation id="id2273414">
          <m:math overflow="scroll" mode="display">
            <m:mrow>
              <m:msub>
                <m:mfenced separators="" open="(" close=")">
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>d</m:mi>
                      <m:msub>
                        <m:mi>x</m:mi>
