                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mrow>
                    <m:mi>ρ</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>D</m:mi>
