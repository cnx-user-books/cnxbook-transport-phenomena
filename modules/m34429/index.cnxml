                    </m:mfrac>
                    <m:mo>+</m:mo>
                    <m:mi>∇</m:mi>
                    <m:mo>•</m:mo>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mrow>
                        <m:mi>ρ</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:mi>F</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:mi mathvariant="bold">v</m:mi>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>-</m:mo>
                    <m:mi>F</m:mi>
                    <m:mfenced separators="" open="[" close="]">
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>∂</m:mi>
                            <m:mi>ρ</m:mi>
                          </m:mrow>
