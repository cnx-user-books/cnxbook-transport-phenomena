                    <m:mo>=</m:mo>
                    <m:mi>ρ</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi>F</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi>∇</m:mi>
                    <m:mo>•</m:mo>
                    <m:mi mathvariant="bold">v</m:mi>
                    <m:mo>+</m:mo>
                    <m:mi>F</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi mathvariant="bold">v</m:mi>
                    <m:mo>•</m:mo>
                    <m:mi>∇</m:mi>
                    <m:mi>ρ</m:mi>
                    <m:mo>+</m:mo>
                    <m:mi>ρ</m:mi>
                    <m:mspace width="0.166667em"/>
