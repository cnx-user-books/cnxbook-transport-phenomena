                  <m:mrow>
                    <m:msup>
                      <m:mi>∇</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:msub>
                      <m:mi>U</m:mi>
                      <m:mi>n</m:mi>
                    </m:msub>
