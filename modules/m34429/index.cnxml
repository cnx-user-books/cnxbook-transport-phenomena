              <m:mi>x</m:mi>
              <m:mfenced separators="" open="(" close=")">
                <m:mrow>
                  <m:msub>
                    <m:mi>S</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo>-</m:mo>
                  <m:msub>
                    <m:mi>S</m:mi>
                    <m:mn>1</m:mn>
                  </m:msub>
                </m:mrow>
              </m:mfenced>
