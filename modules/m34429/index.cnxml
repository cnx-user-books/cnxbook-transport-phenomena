                    <m:mi>ρ</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>F</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>+</m:mo>
                    <m:mi>F</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
