                    <m:mo>•</m:mo>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mrow>
                        <m:mi>ρ</m:mi>
                        <m:mspace width="0.166667em"/>
                        <m:mi mathvariant="bold">v</m:mi>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>=</m:mo>
                    <m:mn>0</m:mn>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mi> continuity </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> equation </m:mi>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mrow>
                    <m:mi>ρ</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>D</m:mi>
                        <m:mi>F</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>D</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>ρ</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>F</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>+</m:mo>
                    <m:mi>ρ</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi mathvariant="bold">v</m:mi>
                    <m:mo>•</m:mo>
                    <m:mi>∇</m:mi>
                    <m:mi>F</m:mi>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mi> for </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi>F</m:mi>
                    <m:mo>=</m:mo>
                    <m:mi mathvariant="bold">v</m:mi>
                    <m:mo>,</m:mo>
                    <m:mspace width="0.277778em"/>
                    <m:mi>S</m:mi>
                    <m:mo>,</m:mo>
                    <m:mspace width="0.277778em"/>
                    <m:mi> or </m:mi>
                    <m:mspace width="0.277778em"/>
