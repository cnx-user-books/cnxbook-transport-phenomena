                    <m:mspace width="1.em"/>
                    <m:mi> velocity </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> perpendicular </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> to </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> velocity </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> gradient </m:mi>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd/>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi mathvariant="bold">v</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
                    <m:mi>ν</m:mi>
                    <m:mspace width="0.166667em"/>
