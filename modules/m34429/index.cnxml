                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  
                    <m:msup>
                      <m:mi>R</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
</m:mtd>
<m:mtd>                  
  <m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">       
