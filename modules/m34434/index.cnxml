                    <m:mi>e</m:mi>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mi>h</m:mi>
                      <m:mi>b</m:mi>
                    </m:mfrac>
                    <m:mo>-</m:mo>
                    <m:mn>1</m:mn>
                    <m:mo>=</m:mo>
                    <m:mi>β</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
