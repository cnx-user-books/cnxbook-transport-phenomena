                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mi>o</m:mi>
                    </m:msub>
                    <m:mo>=</m:mo>
                    <m:mn>2</m:mn>
                    <m:mi>σ</m:mi>
                    <m:mo>/</m:mo>
