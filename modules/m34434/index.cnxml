                      <m:mi>λ</m:mi>
                      <m:mi>j</m:mi>
                    </m:msubsup>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>ε</m:mi>
                      <m:mrow>
                        <m:mi>λ</m:mi>
                        <m:mi>α</m:mi>
                      </m:mrow>
                    </m:msup>
                    <m:mspace width="0.166667em"/>
                    <m:msub>
                      <m:mi>b</m:mi>
                      <m:mrow>
                        <m:mi>α</m:mi>
                        <m:mi>β</m:mi>
                      </m:mrow>
                    </m:msub>
