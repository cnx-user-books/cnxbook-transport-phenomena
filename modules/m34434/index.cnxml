                        <m:msub>
                          <m:mi>x</m:mi>
                          <m:mn>3</m:mn>
                        </m:msub>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="0.277778em"/>
