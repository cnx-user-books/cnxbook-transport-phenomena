                        <m:mtr>
                          <m:mtd>
                            <m:mrow>
                              <m:mo>±</m:mo>
                              <m:mi>U</m:mi>
                              <m:mspace width="1.em"/>
                              <m:mi> at </m:mi>
                              <m:mspace width="0.277778em"/>
                              <m:msub>
