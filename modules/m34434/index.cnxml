                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mrow>
                        <m:mo>±</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>∂</m:mi>
