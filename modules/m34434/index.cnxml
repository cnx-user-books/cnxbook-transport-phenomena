                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mi> in </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> front </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> of </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mi> bubble </m:mi>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mi> thus </m:mi>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
