                        <m:mn>2</m:mn>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>+</m:mo>
                    <m:mi>γ</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msup>
                      <m:mi>e</m:mi>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mfrac>
                          <m:mn>1</m:mn>
