                    <m:msub>
                      <m:mi>T</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mi>j</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:msub>
                      <m:mi>v</m:mi>
