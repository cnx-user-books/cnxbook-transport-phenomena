       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
   </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:msub>
    <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
    <m:mrow>
     <m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
    </m:mrow>
   </m:msub>
   <m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
    <m:mtable>
     <m:mtr>
      <m:mtd>
       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr>
      <m:mtd>
       <m:mn>1</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr>
      <m:mtd>
       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
   </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:msub>
    <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
    <m:mrow>
     <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo>
    </m:mrow>
   </m:msub>
   <m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
    <m:mtable>
     <m:mtr>
      <m:mtd>
       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr>
      <m:mtd>
       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr>
      <m:mtd>
       <m:mn>1</m:mn>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
   </m:mrow> <m:mo>]</m:mo></m:mrow>
  </m:mrow>
  
 </m:semantics>
</m:math>
</equation>
          
          <para id="id11386567">The suffixes to <emphasis effect="bold">e</emphasis> are enclosed in parentheses to show that they do not denote components. A vector, <emphasis effect="bold">a</emphasis>, can be expressed in terms of its components, (<emphasis effect="italics">a</emphasis><sub><emphasis effect="italics">1</emphasis></sub><emphasis effect="italics">, a</emphasis><sub><emphasis effect="italics">2</emphasis></sub><emphasis effect="italics">,</emphasis><emphasis effect="italics">a</emphasis><sub><emphasis effect="italics">3</emphasis></sub>) and the unit vectors.</para>
          <equation id="eip-236"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">a</m:mi></m:mstyle><m:mo>=</m:mo><m:msub>
    <m:mi>a</m:mi>
    <m:mn>1</m:mn>
   </m:msub>
<m:msub>
    <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
    <m:mrow>
     <m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
    </m:mrow>
   </m:msub>
   <m:mo>+</m:mo><m:msub>
    <m:mi>a</m:mi>
    <m:mn>2</m:mn>
   </m:msub>
<m:msub>
    <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
    <m:mrow>
