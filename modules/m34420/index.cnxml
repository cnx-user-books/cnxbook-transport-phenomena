        <m:mi>b</m:mi>
        <m:mn>1</m:mn>
       </m:msub>
       <m:msub>
        <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
       </m:msub>
       <m:mo>+</m:mo><m:msub>
        <m:mi>b</m:mi>
        <m:mn>2</m:mn>
       </m:msub>
       <m:msub>
        <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
       </m:msub>
       <m:mo>+</m:mo><m:msub>
        <m:mi>b</m:mi>
        <m:mn>3</m:mn>
       </m:msub>
       <m:msub>
        <m:mstyle mathsize="normal" mathvariant="bold"><m:mi mathvariant="bold">e</m:mi></m:mstyle>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
       </m:msub>
       
      </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd/>
<m:mtd>
     <m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">
<m:msub>
      <m:mi>a</m:mi>
      <m:mi>i</m:mi>
     </m:msub>
