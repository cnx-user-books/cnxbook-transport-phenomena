                        </m:msub>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>d</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
                    <m:mn>0</m:mn>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mspace width="1.em"/>
                    <m:mi> if </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mfenced separators="" open="|" close="|">
                      <m:msub>
