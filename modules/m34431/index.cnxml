                    </m:msub>
                    <m:mo>=</m:mo>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msub>
                          <m:mi>τ</m:mi>
                          <m:mrow>
                            <m:mi>θ</m:mi>
                            <m:mi>z</m:mi>
                          </m:mrow>
                        </m:msub>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>z</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
                    <m:mn>0</m:mn>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:msup>
                          <m:mi>∂</m:mi>
                          <m:mn>2</m:mn>
                        </m:msup>
                        <m:msub>
                          <m:mi>v</m:mi>
                          <m:mi>θ</m:mi>
                        </m:msub>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msup>
                          <m:mi>z</m:mi>
                          <m:mn>2</m:mn>
                        </m:msup>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>=</m:mo>
