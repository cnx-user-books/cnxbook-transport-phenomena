                        <m:mi>x</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:mo>=</m:mo>
                    <m:mo>-</m:mo>
                    <m:msub>
                      <m:mi>μ</m:mi>
                      <m:mi>o</m:mi>
                    </m:msub>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>d</m:mi>
                        <m:msub>
                          <m:mi>v</m:mi>
                          <m:mi>x</m:mi>
                        </m:msub>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>d</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mo>±</m:mo>
                    <m:msub>
                      <m:mi>τ</m:mi>
                      <m:mi>o</m:mi>
                    </m:msub>
                    <m:mo>,</m:mo>
                    <m:mspace width="1.em"/>
                    <m:mi> if </m:mi>
                    <m:mspace width="0.277778em"/>
                    <m:mfenced separators="" open="|" close="|">
                      <m:msub>
                        <m:mi>τ</m:mi>
                        <m:mrow>
                          <m:mi>y</m:mi>
