                    </m:mfrac>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msub>
                          <m:mi>x</m:mi>
                          <m:mi>j</m:mi>
                        </m:msub>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msub>
                          <m:mi>ξ</m:mi>
