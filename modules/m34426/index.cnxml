                          <m:mo>∫</m:mo>
                        </m:mpadded>
                        <m:mpadded width="-3pt">
                          <m:mo>∫</m:mo>
                        </m:mpadded>
                        <m:mpadded width="-3pt">
                          <m:mo>∫</m:mo>
                        </m:mpadded>
                        <m:mspace width="0.277778em"/>
                      </m:mrow>
                      <m:mi>v</m:mi>
                    </m:munder>
                    <m:mi>ρ</m:mi>
                    <m:mi mathvariant="bold">a</m:mi>
                    <m:mi>d</m:mi>
                    <m:mi>V</m:mi>
                    <m:mo>=</m:mo>
                    <m:munder>
                      <m:mrow>
                        <m:mspace width="0.277778em"/>
                        <m:mpadded width="-3pt">
