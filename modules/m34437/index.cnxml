              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>w</m:mi>
                        <m:mo>*</m:mo>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
