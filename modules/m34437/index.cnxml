                      <m:mn>1</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>-</m:mo>
                    <m:mi>b</m:mi>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>i</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mn>1</m:mn>
                      <m:mo>,</m:mo>
                      <m:mn>1</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>*</m:mo>
                    <m:msubsup>
                      <m:mi>ψ</m:mi>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>B</m:mi>
                        <m:mi>C</m:mi>
                      </m:mrow>
                    </m:msubsup>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>f</m:mi>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>i</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mn>2</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>=</m:mo>
                    <m:mi>f</m:mi>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>i</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mn>2</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>-</m:mo>
                    <m:mi>b</m:mi>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>i</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mn>2</m:mn>
                      <m:mo>,</m:mo>
                      <m:mn>2</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>*</m:mo>
                    <m:mo>(</m:mo>
                    <m:mn>2</m:mn>
                    <m:mo>*</m:mo>
                    <m:msubsup>
                      <m:mi>ψ</m:mi>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>B</m:mi>
                        <m:mi>C</m:mi>
                      </m:mrow>
                    </m:msubsup>
                    <m:mo>/</m:mo>
                    <m:msup>
                      <m:mi>δ</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mo>-</m:mo>
                    <m:mn>2</m:mn>
                    <m:mo>*</m:mo>
                    <m:msubsup>
                      <m:mi>v</m:mi>
                      <m:mi>v</m:mi>
                      <m:mrow>
                        <m:mi>B</m:mi>
                        <m:mi>C</m:mi>
                      </m:mrow>
                    </m:msubsup>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>j</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                    <m:mo>/</m:mo>
                    <m:mi>δ</m:mi>
                    <m:mo>-</m:mo>
                    <m:mi>α</m:mi>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:msubsup>
                          <m:mi>v</m:mi>
                          <m:mi>x</m:mi>
                          <m:mrow>
                            <m:mi>B</m:mi>
