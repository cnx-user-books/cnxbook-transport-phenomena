                  </m:mrow>
                </m:mtd>
              </m:mtr>
            </m:mtable>
          </m:math>
        </equation>
        <para id="id2261769">The dimensionless equation for vorticity with the * dropped is</para>
        <equation id="id2261774">
          <m:math overflow="scroll" mode="display">
            <m:mrow>
              <m:mi>R</m:mi>
              <m:mi>e</m:mi>
              <m:mspace width="0.166667em"/>
              <m:mfenced separators="" open="[" close="]">
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>∂</m:mi>
                      <m:mi>w</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>∂</m:mi>
                      <m:mi>t</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mi>x</m:mi>
                  </m:msub>
