                    <m:mspace width="0.166667em"/>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                      <m:mi>n</m:mi>
