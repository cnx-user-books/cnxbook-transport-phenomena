                    <m:mo>+</m:mo>
                    <m:mi>b</m:mi>
                    <m:mi>Δ</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msubsup>
                    <m:mo>+</m:mo>
                    <m:mi>c</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi>Δ</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msubsup>
                    <m:mo>+</m:mo>
                    <m:mi>d</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:mi>Δ</m:mi>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
