              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mi> thus </m:mi>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mo>'</m:mo>
                    </m:msup>
                    <m:mo>=</m:mo>
                    <m:msub>
                      <m:mi>f</m:mi>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mo>,</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo>-</m:mo>
                    <m:msub>
                      <m:mi>e</m:mi>
                      <m:mrow>
