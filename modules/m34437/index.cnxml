                      <m:mi>μ</m:mi>
                      <m:mi>ρ</m:mi>
                    </m:mfrac>
                    <m:mfenced separators="" open="[" close="]">
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:msup>
                              <m:mi>∂</m:mi>
