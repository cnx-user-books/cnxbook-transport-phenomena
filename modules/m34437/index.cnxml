                </m:mrow>
                <m:mrow>
                  <m:mi>∂</m:mi>
                  <m:msup>
                    <m:mi>x</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                </m:mrow>
              </m:mfrac>
              <m:mo>+</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:msup>
                    <m:mi>∂</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>P</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mi>∂</m:mi>
