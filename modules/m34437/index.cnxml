                        </m:mfenced>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>⇒</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfrac>
                      <m:mi>α</m:mi>
                      <m:mi>r</m:mi>
                    </m:mfrac>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>∂</m:mi>
                        <m:mfenced separators="" open="(" close=")">
                          <m:mrow>
                            <m:mi>v</m:mi>
                            <m:mspace width="0.166667em"/>
