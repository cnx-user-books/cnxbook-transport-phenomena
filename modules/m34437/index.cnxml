                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msubsup>
                    <m:mo>-</m:mo>
                    <m:mfrac>
                      <m:mrow>
                        <m:msup>
                          <m:mi>δ</m:mi>
                          <m:mn>2</m:mn>
                        </m:msup>
                        <m:mi> Re </m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>Δ</m:mi>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:mfrac>
                    <m:mi>Δ</m:mi>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msubsup>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mo>=</m:mo>
                    <m:mi>f</m:mi>
                    <m:mo>-</m:mo>
                    <m:mi>a</m:mi>
                    <m:mspace width="0.166667em"/>
                    <m:msubsup>
                      <m:mi>w</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                        <m:mo>,</m:mo>
                        <m:mi>j</m:mi>
                      </m:mrow>
                      <m:mi>n</m:mi>
                    </m:msubsup>
                    <m:mo>-</m:mo>
