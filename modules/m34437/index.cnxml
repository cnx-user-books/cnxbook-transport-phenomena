                  </m:msub>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>∂</m:mi>
                      <m:mi>w</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>∂</m:mi>
                      <m:mi>y</m:mi>
                    </m:mrow>
                  </m:mfrac>
                </m:mrow>
              </m:mfenced>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:msup>
                    <m:mi>∂</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>w</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mi>∂</m:mi>
