                    </m:mfrac>
                    <m:mspace width="1.em"/>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mo>'</m:mo>
                    </m:msup>
                    <m:mo>=</m:mo>
                    <m:mi>f</m:mi>
                    <m:mo>-</m:mo>
