                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mfenced separators="" open="[" close="]">
                      <m:mfrac>
                        <m:mrow>
                          <m:mi>μ</m:mi>
                          <m:mspace width="0.166667em"/>
                          <m:msub>
                            <m:mi>t</m:mi>
                            <m:mi>o</m:mi>
                          </m:msub>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>ρ</m:mi>
                          <m:mspace width="0.166667em"/>
                          <m:msubsup>
                            <m:mi>L</m:mi>
